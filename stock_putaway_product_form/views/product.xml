<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="product_normal_form_view_inherit" model="ir.ui.view">
        <field name="name">product.product.form.inherit</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view"/>
        <field name="arch" type="xml">
            <group name="group_lots_and_weight" position="after">
                <group name="product_putaways" string="Fixed Put-aways">
                    <field name="fixed_putaway_strategy_ids" context="{'default_category_id': False, 'default_product_id': active_id}">
                        <tree>
                            <field name="putaway_id" />
                            <field name="fixed_location_id" />
                        </tree>
                        <form>
                            <group>
                                <field name="putaway_id" />
                                <field name="fixed_location_id" />
                                <field name="product_id" readonly="1"/>
                            </group>
                        </form>
                    </field>
                    <field name="category_fixed_putaway_ids">
                        <tree>
                            <field name="putaway_id" />
                            <field name="fixed_location_id" />
                        </tree>
                    </field>
                </group>
            </group>
        </field>
    </record>
</odoo>
