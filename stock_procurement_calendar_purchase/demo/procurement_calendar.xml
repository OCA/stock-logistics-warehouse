<odoo noupdate="0">

    <!-- SUPPLIER 1 - PRODUCT 11-->
    <record model="procurement.calendar.attendance" id="procurement_calendar_supplier_1_attendance_p11_w">
        <field name="name">Procurement Suppliers Calendar Wednesday PRODUCT11</field>
        <field name="procurement_calendar_id" ref="stock_procurement_calendar.procurement_calendar_supplier_1"/>
        <field name="dayofweek">2</field>
        <field name="hour_from">08</field>
        <field name="hour_to">18</field>
        <field name="product_ids" eval="[(4, ref('product.product_product_11')), (4, ref('product.product_product_13'))]"/>
    </record>

    <!-- SUPPLIER 2 - PRODUCT 11-->
