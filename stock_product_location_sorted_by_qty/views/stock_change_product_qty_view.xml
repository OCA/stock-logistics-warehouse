<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>
    <record id="view_change_product_quantity" model="ir.ui.view">
        <field name="model">stock.change.product.qty</field>
        <field name="inherit_id" ref="stock.view_change_product_quantity"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location_id']" position="attributes">
                <attribute name="context">{'stock_change_product_quantity': True, 'default_product_id':product_id}</attribute>
            </xpath>
        </field>
    </record>
</data>
</openerp>
