<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>

    <record id="reception_checkpoint_tree_view" model="ir.ui.view">
        <field name="model">reception.checkpoint</field>
        <field name="arch" type="xml">
            <tree colors="blue: timing == 'toreceive'; orange: timing == 'more'; red: timing == 'inlate'">
                <field name="order_id"/>
                <field name="partner_ref"/>
                <field name="product_id"/>
                <field name="date_planned"/>
                <field name="received_date"/>
                <field name="ordered_qty"/>
                <field name="received_qty"/>
                <field name="diff_qty"/>
                <field name="timing" invisible="1"/>
                <field name="purch_line" invisible="1"/>
            </tree>
        </field>
    </record>


</data>
</openerp>
