<?xml version="1.0" encoding="UTF-8" ?>
<templates id="add_multiple_products_cost_view" xml:space="preserve">
    <t t-extend="ListView.buttons">
        <t t-jquery="div.o_list_buttons" t-operation='append'>
            <t t-if="widget.modelName == 'stock.cost.adjustment.line'">
                <button
                    type="button"
                    class="add_multi_product_adjustment btn btn-primary"
                    style="margin-left:10px;"
                >
                            Add Multiple Products
                </button>
            </t>
        </t>
    </t>
</templates>
