<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="product_supplierinfo_tree_view_stockforecast" model="ir.ui.view">
            <field name="model">product.supplierinfo</field>
            <field name="inherit_id" ref="product.product_supplierinfo_tree_view" />
            <field name="arch" type="xml">
                <field name="min_qty" position="after" >
                    <field name="stock_period_min"/>
                    <field name="purchase_multiple"/>
                </field>
            </field>
        </record>

        <record
            id="product_supplierinfo_form_view_stockforecast"
            model="ir.ui.view"
            >
            <field name="model">product.supplierinfo</field>
            <field
                name="inherit_id"
                ref="product.product_supplierinfo_form_view"
                />
            <field name="arch" type="xml">
                <field name="delay" position="after" >
                    <field name="stock_period_min"/>
                    <field name="purchase_multiple"/>
                </field>
            </field>
        </record>

    </data>
</openerp>
